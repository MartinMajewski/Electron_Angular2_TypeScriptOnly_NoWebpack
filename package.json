{
  "name": "cap_liper",
  "version": "1.1.0",
  "repository": "./.git",
  "license": "MIT",
  "main": "",
  "scripts": {
    "package": "./node_modules/electron-packager/cli.js dist CapLiper --platform=win32 --arch=all --out=dist --overwrite --version='1.2.5'",
    "pack-mac": "./node_modules/electron-packager/cli.js dist CapLiper.app --platform=darwin",
    "serialportRebuild": "concurrently --raw \"rm -f './node_modules/serialport/build/Release/serialport.node'\"   \"./node_modules/.bin/electron-rebuild\"  ",
    "copyIndexHTML": "copyfiles -u 1 './src/**/index.html' './dist/'  ",
    "copyPackageJSON": "copyfiles -u 1 './src/package.json' './dist/'  ",
    "tsc": "./node_modules/.bin/tsc",
    "copyNonTs": "npm run copyIndexHTML && npm run copyPackageJSON",
    "electron": "./node_modules/.bin/electron ./dist",
    "build": "npm run tsc && npm run copyNonTs && npm run electron",
    "start": "npm run copyNonTs && npm run electron"
  },
  "devDependencies": {
    "@types/electron": "^1.4.29",
    "@types/node": "^6.0.52",
    "@types/redux-logger": "^2.6.32",
    "@types/serialport": "^4.0.6",
    "@types/zone.js": "0.0.27",
    "concurrently": "^3.1.0",
    "devtron": "^1.4.0",
    "electron": "^1.4.12",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.4.0",
    "electron-rebuild": "^1.4.0",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.2",
    "node-sass": "^3.8.0",
    "reflect-metadata": "^0.1.8",
    "spectron": "^3.4.0",
    "tslint": "^4.1.1",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/platform-server": "^2.3.1",
    "@angular/router": "^3.3.1",
    "core-js": "^2.4.1",
    "min-jquery": "^1.7.0",
    "ng2-redux": "^4.2.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "rxjs": "^5.0.1",
    "serialport": "^4.0.7",
    "zone.js": "^0.7.2"
  }
}
